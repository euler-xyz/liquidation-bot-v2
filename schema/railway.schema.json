                            "phases": {
                              "$ref": "#/properties/build/properties/nixpacksPlan/anyOf/0/anyOf/1/properties/phases"
                            }
                          },
                          "additionalProperties": true,
                          "description": "Full nixpacks plan. See https://nixpacks.com/docs/configuration/file for more info"
                        }
                      ]
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "nixpacksVersion": {
                  "anyOf": [
                    {
                      "anyOf": [
                        {
                          "not": {}
                        },
                        {
                          "$ref": "#/properties/build/properties/nixpacksVersion/anyOf/0/anyOf/1"
                        }
                      ]
                    },
                    {
                      "type": "null"
                    }
                  ]
                }
              },
              "additionalProperties": false,
              "description": "Configuration for the build step"
            },
            "deploy": {
              "type": "object",
              "properties": {
                "startCommand": {
                  "anyOf": [
                    {
                      "anyOf": [
                        {
                          "not": {}
                        },
                        {
                          "$ref": "#/properties/deploy/properties/startCommand/anyOf/0/anyOf/1"
                        }
                      ]
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "preDeployCommand": {
                  "anyOf": [
                    {
                      "anyOf": [
                        {
                          "not": {}
                        },
                        {
                          "$ref": "#/properties/deploy/properties/preDeployCommand/anyOf/0/anyOf/1"
                        }
                      ]
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "numReplicas": {
                  "anyOf": [
                    {
                      "anyOf": [
                        {
                          "not": {}
                        },
                        {
                          "$ref": "#/properties/deploy/properties/numReplicas/anyOf/0/anyOf/1"
                        }
                      ]
                    },
                    {
                      "type": "null"
