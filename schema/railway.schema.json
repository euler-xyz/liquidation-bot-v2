                      "anyOf": [
                        {
                          "not": {}
                        },
                        {
                          "$ref": "#/properties/deploy/properties/numReplicas/anyOf/0/anyOf/1"
                        }
                      ]
                    },
                    {
                      "type": "null"
                    }
